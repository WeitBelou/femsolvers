{
  "solver": {
    "type": "elasticity",
    "output": {
      "root": "results/elasticity_cylinder"
    }
  },
  "geometry": {
    "type": "cylinder",
    "height": 5.0,
    "radius": 10.0,
    "resolution": 10
  },
  "boundary_conditions": {
    "dirichlet": [
      {
        "value": [0.0, 0.0, 0.0],
        "marker": {
          "type": "plane",
          "axis": "z",
          "center": [0.0, 0.0, 0.0]
        }
      }
    ],
    "neumann": [
      {
        "value": 1.0e+5,
        "marker": {
          "type": "plane",
          "axis": "z",
          "center": [0.0, 0.0, 5.0]
        }
      }
    ]
  }
}